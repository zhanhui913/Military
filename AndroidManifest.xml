<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.apollo"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="16" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.apollo.Home"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.apollo.Recruit"
            android:label="@string/title_activity_recruit" >
        </activity>
        <activity
            android:name="com.example.apollo.CommandBaseB"
            android:label="@string/title_activity_command_base_b" >
        </activity>
        <activity
            android:name="com.example.apollo.RecruitInfo"
            android:label="@string/title_activity_recruit_info" >
        </activity>
        <activity
            android:name="com.example.apollo.CommandBaseA"
            android:label="@string/title_activity_command_base" >
        </activity>
    </application>

</manifest>